def remove_every_other(my_list):
    res = []
    for i in range(0, len(my_list), 2):
        print(i)
        res.append(my_list[i])
    return res
_____________________________________________________
def remove_every_other(my_list):
    return [x for i,x in enumerate(my_list) if i%2 == 0]

