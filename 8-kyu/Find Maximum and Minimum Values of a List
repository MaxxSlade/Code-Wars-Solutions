int min(int* array, int arrayLength)
{
  int i;
  int min = array[0];
  for(i = 0; i < arrayLength; i++){
    int num = array[i];
    if(num < min){
      min = num;
    }
  }
  
  return min;
}

int max(int* array, int arrayLength)
{
  int i;
  int max = array[0];
  for(i = 0; i < arrayLength; i++){
    int num = array[i];
    if(num > max){
      max = num;
    }
  }
  
  return max;
}
