def check_exam(arr1,arr2):
    result = 0
    for x in range(len(arr1)):
        if arr1[x] == arr2[x]:
            result += 4
        elif arr1[x] != arr2[x] and arr2[x] != '':
            result -= 1
    return result if result > 0 else 0

# https://www.codewars.com/kata/5a3dd29055519e23ec000074/python
