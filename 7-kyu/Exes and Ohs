def xo(s):
    xc = 0
    oc = 0
    for x in s:
        if x.lower() == 'o':
            oc += 1
        elif x.lower() == 'x':
            xc += 1
    
    if xc == oc:
        return True
    else:
        return False
        
# very ugly but I first tried it
# https://www.codewars.com/kata/55908aad6620c066bc00002a/python
