def get_sum(a,b):
    arr = []
    if a > b:
        for i in range(b, a+1):
            arr.append(i)
    elif b > a:
        for x in range(a, b+1):
            arr.append(x)
    else:
        return a
    return sum(arr)
---------------------------------------------
def get_sum(a,b):
    return sum([x for x in range(a, b+1)]) if b > a else sum([x for x in range(b, a+1)])

